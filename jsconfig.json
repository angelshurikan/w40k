{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "checkJs": true,
    "allowJs": true,
    "types": [
      "@league-of-foundry-developers/foundry-vtt-types"
    ]
  },
  "include": [
    "**/*.js",
    "**/*.mjs",
    "**/*.cjs"
  ],
  "exclude": [
    "node_modules",
    "packs"
  ]
}

